<?xml version='1.0' encoding='UTF-8'?>
<package uuid="facialrecognition-abb3bd" version="0.0.0">
 <names>
  <name lang="en_US">FacialRecognition</name>
 </names>
 <supportedLanguages>
  <language>en_US</language>
 </supportedLanguages>
 <descriptionLanguages>
  <language>en_US</language>
 </descriptionLanguages>
 <contents>
  <behaviorContent path="behavior_1">
   <userRequestable/>
   <nature>interactive</nature>
   <names>
    <name lang="en_US">RecognizeFaces</name>
   </names>
   <descriptions>
    <description lang="en_US">NAO recognizes peoples face.</description>
   </descriptions>
   <tags>
    <tag lang="en_US">recognition</tag>
    <tag lang="en_US">recognize</tag>
    <tag lang="en_US">face</tag>
    <tag lang="en_US">people</tag>
   </tags>
   <triggerSentences>
    <sentence lang="en_US">Have we met before?</sentence>
    <sentence lang="en_US">Do you remember me?</sentence>
   </triggerSentences>
   <loadingResponses>
    <sentence lang="en_US">I remember you!</sentence>
   </loadingResponses>
   <autonomous>
    <condition purpose="launchTrigger"><![CDATA[(('Launchpad/NumPeopleZone2' > 0) ~ 5)
&& ('Launchpad/PreviousActivity' !="facialrecognition-abb3bd")]]></condition>
   </autonomous>
   <permissions/>
  </behaviorContent>
  <behaviorContent path="LearnFace">
   <nature>interactive</nature>
   <triggerSentences>
    <sentence lang="en_US">Learn my face</sentence>
   </triggerSentences>
   <loadingResponses>
    <sentence lang="en_US">Okay. I'm learning your face.</sentence>
   </loadingResponses>
   <autonomous/>
   <permissions/>
  </behaviorContent>
 </contents>
</package>
